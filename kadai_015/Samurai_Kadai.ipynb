        "class Human:\n",
        "    # コンストラクタを定義する\n",
        "    def __init__(self, name, age):\n",
        "        self.name = name\n",
        "        self.age = age\n",
        "\n",
        "    def printinfo(self):\n",
        "        print(self.name)\n",
        "        print(self.age)\n",
        "\n",
        "# インスタンス化する\n",
        "person_a = Human(\"Alex\", 37)\n",
        "\n",
        "# 属性にアクセスし、値を出力する\n",
        "person_a.printinfo()"
