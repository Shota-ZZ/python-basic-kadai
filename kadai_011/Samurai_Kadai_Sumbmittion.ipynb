      "source": [
        "# forを使用して1個ずつ名称を取り出す\n",
        "array = [\"水\",\"金\",\"地\",\"火\",\"木\",\"土\",\"天\",\"海\",\"冥\"]\n",
        "\n",
        "for i in array:\n",
        "  print(i)\n",
        "\n",
        "# whileを使用して1個ずつ名称を取出す\n",
        "array = [\"水\",\"金\",\"地\",\"火\",\"木\",\"土\",\"天\",\"海\",\"冥\"]\n",
        "\n",
        "i = 0\n",
        "while i <= len(array)-1:\n",
        "  print(array[i])\n",
        "  i += 1\n",
        "\n",
        "\n",
        "\n"
      ],
