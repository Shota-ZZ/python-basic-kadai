        "# 関数の引数として商品金額と税率を入力\n",
        "def total_price(price, tax):\n",
        "  total_amount = price + price * tax\n",
        "  return total_amount\n",
        "\n",
        "# 関数の戻り値を変数に保存\n",
        "result = total_price(100, 0.1)\n",
        "# 計算結果を関数の外に出力\n",
        "print(result)"
